{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "license": "ISC",
  "author": "",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc --p tsconfig.prod.json && npm run copy-files",
    "copy-files": "cp ./src/openapi.json ./dist/",
    "db:clean": "prisma migrate reset --force",
    "db:generate": "prisma generate",
    "db:migrate:development": "dotenv -e .env.development -- npx prisma migrate dev",
    "db:migrate:production": "dotenv -e .env -- npx prisma migrate dev",
    "db:migrate:test": "dotenv -e .env.test -- npx prisma migrate dev",
    "db:seed:development": "cross-env NODE_ENV=development ts-node prisma/seed.ts",
    "db:seed:production": "cross-env NODE_ENV=production ts-node prisma/seed.ts",
    "db:seed:test": "cross-env NODE_ENV=test ts-node prisma/seed.ts",
    "db:studio": "prisma studio",
    "format": "npm run format:package && npm run format:prettier && npm run format:code",
    "format:code": "eslint --ext .ts,.json src/ --ext .ts,.json types/ --ext .ts,.json prisma/ --ext .ts,.json test/ --fix",
    "format:package": "sort-package-json",
    "format:prettier": "prettier --write \"src/**/*.{ts,tsx,json}\" \"types/**/*.{ts,tsx,json}\" \"prisma/**/*.{ts,tsx,json}\" \"test/**/*.{ts,tsx,json}\"",
    "lint": "npm run lint:ts && npm run lint:code",
    "lint:code": "eslint --ext .ts,.json src/ --ext .ts,.json types/ --ext .ts,.json prisma/ --ext .ts,.json test/",
    "lint:commit": "commitlint -e",
    "lint:ts": "tsc --noEmit",
    "pre-commit": "lint-staged",
    "prepare": "husky install",
    "start": "npm run start:dev",
    "start:debug": "cross-env NODE_ENV=development nodemon --inspect ./src/index.ts",
    "start:dev": "cross-env NODE_ENV=development nodemon ./src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node --experimental-specifier-resolution=node dist/index.js",
    "swagger:generate": "ts-node swagger.generate.ts",
    "test": "cross-env NODE_ENV=test node --experimental-vm-modules --experimental-specifier-resolution=node node_modules/jest/bin/jest.js",
    "test:coverage": "cross-env NODE_ENV=test node --experimental-vm-modules --experimental-specifier-resolution=node node_modules/jest/bin/jest.js --coverage",
    "test:e2e": "npm run db:clean && cross-env NODE_ENV=test node --experimental-vm-modules --experimental-specifier-resolution=node node_modules/jest/bin/jest.js --config jest.e2e.config.ts",
    "test:e2e:coverage": "npm run db:clean && cross-env NODE_ENV=test node --experimental-vm-modules --experimental-specifier-resolution=node node_modules/jest/bin/jest.js --config jest.e2e.config.ts --coverage"
  },
  "dependencies": {
    "@koa/cors": "^3.3.0",
    "@koa/multer": "^3.0.0",
    "@koa/router": "^10.1.1",
    "@prisma/client": "^3.13.0",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.0",
    "inversify": "^6.0.1",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-helmet": "^7.0.2",
    "koa-logger": "^3.2.1",
    "koa2-swagger-ui": "^5.5.0",
    "multer": "^1.4.5-lts.1",
    "proper-url-join": "^2.1.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "server-destroy": "^1.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.3",
    "tslog": "^4.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@jest/globals": "^29.3.1",
    "@types/bcrypt": "^5.0.0",
    "@types/debug": "^4.1.7",
    "@types/jest": "^29.2.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/koa": "^2.13.4",
    "@types/koa-bodyparser": "^4.3.7",
    "@types/koa-logger": "^3.1.2",
    "@types/koa__cors": "^3.3.0",
    "@types/koa__router": "^8.0.11",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.27",
    "@types/proper-url-join": "^2.1.1",
    "@types/server-destroy": "^1.0.1",
    "@types/source-map-support": "^0.5.4",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/validator": "^13.7.2",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^5.1.0",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-check-file": "^1.2.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-perfectionist": "^2.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-regexp": "^2.1.1",
    "eslint-plugin-unicorn": "^43.0.1",
    "husky": "^8.0.1",
    "jest": "^29.3.1",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.6.2",
    "prisma": "^3.13.0",
    "sort-package-json": "^1.57.0",
    "swagger-jsdoc": "^6.2.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=18"
  }
}
